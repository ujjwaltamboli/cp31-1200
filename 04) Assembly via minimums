#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        int n1=n;
        int n2=(n*(n-1))/2;
        vector<int> store(n2);
        for (int i=0;i<n2;i++){
            cin>>store[i];
        }
        sort(store.begin(), store.end());
        vector<int> ans;
        for (int i=0;i<n2;i=i+n){
            ans.push_back(store[i]);
            n--;
        }
        while (ans.size()<n1){
            ans.push_back(store.back());
        }

        for (int i=0;i<n1;i++){
            cout<<ans[i]<<" ";
        }
        cout<<endl;

        //T.C.:- O(n);
        //S.C.:- O(a+b);
    }
    return 0;
}
